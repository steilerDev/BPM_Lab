<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:FlowSOG="http://iaas.uni-stuttgart.de/labs/FlowSOG" xmlns:pns="http://de.ustutt.simtech">
  <process name="pns:myProcess">
    <metaData>
      <mdProperty name="myProperty" value="myPropertyValue"/>
    </metaData>
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="pns:myProcessService" port="myProcessPort"/>
    </provide>
    <invoke partnerLink="InventoryPartnerLink">
      <service name="FlowSOG:InventoryService" port="InventoryPort"/>
    </invoke>
    <invoke partnerLink="OrderPartnerLink">
      <service name="FlowSOG:OrderService" port="OrderPort"/>
    </invoke>
    <invoke partnerLink="ShipmentPartnerLink">
      <service name="FlowSOG:ShipmentService" port="ShipmentPort"/>
    </invoke>
    <invoke partnerLink="PaymentPartnerLink">
      <service name="FlowSOG:PaymentService" port="PaymentPort"/>
    </invoke>
  </process>
</deploy>