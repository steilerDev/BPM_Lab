<!-- PlaygroundProcess BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Sat Dec 03 22:15:53 CET 2016 -->
<bpel:process name="PlaygroundProcess"
         targetNamespace="http://de.group-one"
         suppressJoinFailure="yes"
         xmlns:tns="http://de.group-one"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns="http://iaas.uni-stuttgart.de/labs/FlowSOG">

    <!-- Import the client WSDL -->
	<bpel:import location="PlaygroundProcessArtifacts.wsdl" namespace="http://de.group-one" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:PlaygroundProcess"
                     myRole="PlaygroundProcessProvider"
                     />
        <bpel:partnerLink name="ProductListPL" partnerLinkType="tns:ProductListPLT" partnerRole="InventoryPLRole"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:PlaygroundProcessRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:PlaygroundProcessResponseMessage"/>
        <bpel:variable name="ProductListPLResponse" messageType="ns:listProductsOutput"></bpel:variable>
        <bpel:variable name="ProductListPLRequest" messageType="ns:listProductsInput"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in PlaygroundProcess.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:PlaygroundProcess"
                 operation="process" variable="input"
                 createInstance="yes"/>
        <bpel:assign validate="no" name="AssignInput">
            <bpel:copy>
                <bpel:from><bpel:literal xml:space="preserve"><tns:listProducts xmlns:tns="http://iaas.uni-stuttgart.de/labs/FlowSOG" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:limit>10</tns:limit>
</tns:listProducts>
</bpel:literal></bpel:from>
                <bpel:to variable="ProductListPLRequest" part="listProductsInput"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input]]></bpel:query>
                </bpel:from>
                <bpel:to part="listProductsInput" variable="ProductListPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:limit]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeProductList" partnerLink="ProductListPL" operation="listProducts" portType="ns:InventoryPortType" inputVariable="ProductListPLRequest" outputVariable="ProductListPLResponse"></bpel:invoke>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="AssignListOutput">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:PlaygroundProcessResponse xmlns:tns="http://de.group-one" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:PlaygroundProcessResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="listProductsOutput" variable="ProductListPLResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:productList/ns:product[1]/ns:name]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:PlaygroundProcess"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

